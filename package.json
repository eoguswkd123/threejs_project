        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^13.24.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4.1.12",
        "typescript": "^5.6.3",
