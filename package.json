        "@types/react-dom": "^18.3.1",
        "@types/three": "^0.181.0",
        "@vitejs/plugin-react": "^4.3.2",
        "@vitest/coverage-v8": "^3.2.4",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.39.1",
