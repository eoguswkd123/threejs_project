        "axios": "^1.13.2",
        "clsx": "^2.1.1",
        "dxf-parser": "^1.1.2",
        "lucide-react": "^0.542.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.62.0",
        "react-router-dom": "^6.30.1",
        "tailwind-merge": "^3.3.1",
        "three": "^0.181.2",
        "zod": "^4.1.5",
        "zustand": "^5.0.8"
